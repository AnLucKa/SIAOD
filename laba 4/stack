# задание 11
def computeForm(str):
    x=1
    y=1
    z=1
    stk= Stack()
    str1=""
    for i in range(len(str) - 1, -1, -1):
        stk.push(str[i])
    for i in range(len(str)):
        str1+=stk.pop()
    try:
        eval(str1)
    except:
        print("False")
        return
    print("True")
computeForm("2 +(3+)")
