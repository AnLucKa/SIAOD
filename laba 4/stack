# задание 10
def computeMinMax(str):
    str1=""
    stk= Stack()
    for i in range (len(str)-1, -1, -1):
        stk.push(str[i])
    for i in range(len(str)):
        if(stk.peek()=="0"):
            str1+="0"
        if(stk.peek()=="1"):
            str1+="1"
        if(stk.peek()=="2"):
            str1+="2"
        if(stk.peek()=="3"):
            str1+="3"
        if(stk.peek()=="4"):
            str1+="4"
        if(stk.peek()=="5"):
            str1+="5"
        if(stk.peek()=="6"):
            str1+="6"
        if(stk.peek()=="7"):
            str1+="7"
        if(stk.peek()=="8"):
            str1+="8"
        if(stk.peek()=="9"):
            str1+="9"
        if(stk.peek()=="M"):
            str1+="max"
        if(stk.peek()=="N"):
            str1+="min"
        if(stk.peek()==","or stk.peek()=="."):
            str1+=","
        if(stk.peek()=="("):
            str1+="("
        if(stk.peek()==")"):
            str1+=")"
        stk.pop()
    print(eval(str1))
computeMinMax("N(3,5)")


