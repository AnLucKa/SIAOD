# задание 9
def computeLogic (str):
    str1=""
    stk= Stack()
    for i in range (len(str)):
        stk.push(str[i])
    for i in range (len(str)):
        if(stk.peek()=="T"):
            str1 +="True "
        if(stk.peek()=="F"):
            str1+="False "
        if(stk.peek()=="N"):
            str1 += "Not is "
        if(stk.peek()=="A" or stk.peek()=="*"):
            str1+="and "
        if(stk.peek()=="X"):
            str1="!= " +str1
        if(stk.peek()=="O" or stk.peek()=="+"):
            str1+="or "
        if(stk.peek()=="("):
            str1+="( "
        if(stk.peek()==")"):
            str1+=")"
        stk.pop()
    print(eval(str1))
computeLogic("TAF")

print()
